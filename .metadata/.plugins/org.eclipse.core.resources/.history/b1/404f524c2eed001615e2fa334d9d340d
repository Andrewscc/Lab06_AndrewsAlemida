
public class Jogo {

	private String nome;
	private double preco;
	private int maiorScore;
	private int qtdJogadas;
	private int qtdZermentos;

	public Jogo(String nome, double preco) {
		super();
		this.nome = nome;
		this.preco = preco;
		this.maiorScore = 0;
		this.qtdJogadas = 0;
		this.qtdZermentos = 0;
	}

}
